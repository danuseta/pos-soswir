{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "optimize": "npm run check && npm run build:prod",
    "serve": "vite preview --host 0.0.0.0 --port 3000",
    "clean": "rm -rf .svelte-kit build node_modules/.vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check .",
    "clean:code": "npm run lint:fix && npm run format",
    "check:all": "npm run check && npm run lint && npm run format:check",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@sveltejs/adapter-netlify": "^2.0.8",
    "@sveltejs/kit": "^1.25.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "autoprefixer": "^10.4.14",
    "bits-ui": "^0.22.0",
    "clsx": "^2.0.0",
    "lucide-svelte": "^0.274.0",
    "postcss": "^8.4.24",
    "svelte": "^4.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwind-variants": "^0.1.14",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9",
    "axios": "^1.9.0",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "eslint": "^9.28.0",
    "svelte-chartjs": "^3.1.5",
    "svelte-routing": "^2.13.0",
    "svelte-spa-router": "^4.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "svelte-check": "^3.5.0",
    "terser": "^5.41.0",
    "typescript": "^5.0.0",
    "vite-plugin-imagemin": "^0.6.1"
  }
}
